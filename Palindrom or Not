public class Main {
    public static void main(String[] args) {
        int n = 121;
        int num = 0;
        int n1 = n;

        while (n > 0) {
            int LD = n % 10;      
            num = (num * 10) + LD;
            n = n / 10;            
        }

        if (num == n1) {
            System.out.println("This is a palindrome number: "+num);
        } else {
            System.out.println("This is not a palindrome number: "+num);
        }
    }
}
